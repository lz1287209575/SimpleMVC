<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1"/>
    <httpModules>
      <add name="UrlRoutingModule" type="Simple.Routing.UrlRoutingModule"/>
    </httpModules>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <modules>
      <add name="UrlRoutingModule" type="Simple.Routing.UrlRoutingModule"/>
    </modules>
  </system.webServer>
</configuration>
